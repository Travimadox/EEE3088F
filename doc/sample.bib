	publisher = {Association for Computing Machinery},
	author = {Lee, Dongchi and Saito, Kazuya and Umedachi, Takuya and Ta, Tung D. and Kawahara, Yoshihiro},
	month = oct,
	year = {2018},
	keywords = {Flexible printed circuit board, Origami robot, Soft robot},
	pages = {392--395},
}

